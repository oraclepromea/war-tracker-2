# War Tracker 2.0 Environment Configuration
# Copy this file to .env and update values as needed

# Database Configuration (Optional - will run without database)
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=wartracker
DB_PASSWORD=password
DB_NAME=wartracker_db
DATABASE_URL=your_postgres_database_url

# API Keys (Optional - will use RSS feeds without these)
NEWSAPI_KEY=your_newsapi_key_here
ACLED_API_KEY=your_acled_api_key_here
ACLED_EMAIL=your_email@example.com
OPENROUTER_API_KEY=your_openrouter_api_key

# Server Configuration
PORT=3001
NODE_ENV=development

# Security
JWT_SECRET=your_jwt_secret_here

# Enable/Disable Features
ENABLE_DATABASE=false
ENABLE_API_SOURCES=false
ENABLE_RSS_ONLY=true

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

